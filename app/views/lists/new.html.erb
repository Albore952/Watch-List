<div class="container">
  <h1> You can create a list here</h1>
    <p>
    <%= link_to lists_path do %>
    <i class="fas fa-arrow-left"> Get back on list view</i>
    <% end %>
  </p>
  <%= simple_form_for(@list) do |f| %>
    <%= f.input :name %>
    <br>
    <%= f.input :photo, as: :file %>
    <br>
    <%= f.submit "Submit list", class: "btn btn-primary" %>
  <% end %>
</div>
